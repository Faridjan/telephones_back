{
  "description": "Slim starter + service",
  "name": "autokz/sevice_name",
  "minimum-stability": "stable",
  "license": "proprietary",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.4.0",
    "ext-json": "*",
    "ext-openssl": "^7.4",
    "autokz/oauth_proxy": "^1.0",
    "doctrine/migrations": "^3.0",
    "doctrine/orm": "^2.8",
    "laminas/laminas-config-aggregator": "^1.4",
    "monolog/monolog": "^2.1",
    "php-di/php-di": "^6.3",
    "ramsey/uuid": "^4.1",
    "sentry/sdk": "^3.1",
    "slim/psr7": "^1.3",
    "slim/slim": "^4.7",
    "symfony/console": "^5.2",
    "symfony/validator": "^5.2",
    "webmozart/assert": "^1.9"
  },
  "require-dev": {
    "doctrine/data-fixtures": "^1.4",
    "lctrs/psalm-psr-container-plugin": "^1.1",
    "overtrue/phplint": "^2.3",
    "phpunit/phpunit": "^9.5",
    "psalm/plugin-phpunit": "^0.15.0",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.5",
    "vimeo/psalm": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Farid",
      "email": "farid-n@auto.kz",
      "homepage": "https://github.com/faridjan"
    },
    {
      "name": "Roman Tyan",
      "email": "roman-t@auto.kz",
      "homepage": "https://github.com/tyanrv"
    }
  ],
  "scripts": {
    "check": [
      "@lint",
      "@cs-check"
    ],
    "post-install-cmd": [],
    "app": "php bin/app.php --ansi",
    "phpcs": "phpcs --standard=phpcs.xml",
    "phpcbf": "phpcbf",
    "lint": "phplint",
    "psalm": "psalm --config=psalm.xml",
    "test": "phpunit --colors=always",
    "test-coverage": "XDEBUG_MODE=coverage phpunit --colors=always --coverage-html var/coverage"
  }
}
